{
  "$schema-template": "4.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "speech": {
                "username": "$CONTAINER_REGISTRY_USERNAME_speech",
                "password": "$CONTAINER_REGISTRY_PASSWORD_speech",
                "address": "rgleannmaria.azurecr.io"
              },
              "yourregistryname": {
                "username": "$CONTAINER_REGISTRY_USERNAME_yourregistryname",
                "password": "$CONTAINER_REGISTRY_PASSWORD_yourregistryname",
                "address": "$CONTAINER_REGISTRY_LOGINSERVER_yourregistryname"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.2",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.2",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "InvokeModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.InvokeModule}",
              "createOptions": {}
            },
            "env": {
              "CONNECTION_STRING": {
                "value": "$CONNECTION_STRING"
              },
              "DEVICE_ID": {
                "value": "$DEVICE_ID"
              }
            }
          },
          "azureeyemodule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureedgedevices/azureeyemodule:2112-1",
              "createOptions": {
                "ExposedPorts": {
                  "8554/tcp": {}
                },
                "HostConfig": {
                  "Binds": [
                    "/dev/bus/usb:/dev/bus/usb"
                  ],
                  "Privileged": true,
                  "DeviceCgroupRules": [
                    "c 189:* rmw"
                  ],
                  "PortBindings": {
                    "8554/tcp": [{"HostPort": "8554"}]
                  }
                }
              }
            }
          },
          "EarSomSpeechModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "rgleannmaria.azurecr.io/earsomspeechmodule:0.0.1-arm64v8",
              "createOptions": {
                "HostConfig": {
                  "Binds": [
                    "/dev:/dev",
                    "/tmp/ear_som_modules_data:/app/data"
                  ],
                  "DeviceCgroupRules": [
                    "c 189:* rmw",
                    "c 116:* rmw",
                    "c 509:* rmw",
                    "c 237:* rmw"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "azureeyemodule": {
      "properties.desired": {
        "Logging": true,
        "ModelZipUrl": "https://aedsamples.blob.core.windows.net/vision/aeddevkitnew/person-detection-retail-0013.zip",
        "RawStream": true,
        "ResultStream": true,
        "Running": true,
        "TelemetryInterval": 10
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "routes": {
          "AzureEyeToInovkeModule": "FROM /messages/modules/azureeyemodule/outputs/* INTO BrokeredEndpoint(\"/modules/InvokeModule/inputs/eyeInput\")",
          "InvokeModuleToHub": "FROM /messages/modules/InvokeModule/outputs/* INTO $upstream",
          "EarSomSpeechModuleToIoTHub": "FROM /messages/modules/EarSomSpeechModule/outputs/* INTO $upstream"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}